{
  "compilerOptions": {
    "target": "es2022",
    "useDefineForClassFields": true,
    "module": "es2022",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve"
  },
  "angularCompilerOptions": {
    "strictTemplates": true
  }
}